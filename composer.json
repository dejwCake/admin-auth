{
    "name": "dejwcake/admin-auth",
    "description": "Authentication interface for brackets/admin-ui",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "auth",
        "authentication",
        "login",
        "user",
        "admin",
        "cms",
        "laravel"
    ],
    "authors": [
        {
            "name": "David Běhal",
            "email": "david.behal@dejw.sk",
            "homepage": "https://www.dejw.sk",
            "role": "Developer"
        },
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "dejwcake/admin-ui": "^1.0",
        "dejwcake/craftable-media": "^1.0",
        "illuminate/support": "^12.0",
        "spatie/laravel-permission": "^6.15"
    },
    "require-dev": {
        "larastan/larastan": "^3.1",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.5.9"
    },
    "autoload": {
        "psr-4": {
            "Brackets\\AdminAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\AdminAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\AdminAuth\\AdminAuthServiceProvider"
            ]
        }
    },
    "branch-alias": {
        "dev-main": "1.0-dev"
    }
}
