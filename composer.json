{
    "name": "dejwcake/admin-auth",
    "description": "Authentication interface for brackets/admin-ui",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "auth",
        "authentication",
        "login",
        "user",
        "admin",
        "cms",
        "laravel"
    ],
    "authors": [
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "David Běhal",
            "email": "david.behal@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "dejwcake/admin-ui": "dev-main",
        "dejwcake/craftable-media": "dev-main",
        "illuminate/support": "^10.0",
        "spatie/laravel-permission": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~8.5",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Brackets\\AdminAuth\\": "src/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\AdminAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\AdminAuth\\AdminAuthServiceProvider"
            ]
        }
    },
    "branch-alias": {
        "dev-master": "1.0"
    }
}
